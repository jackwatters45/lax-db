{
  "name": "@lax-db/core",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./*": "./src/*.ts"
  },
  "scripts": {
    "db": "sst shell drizzle-kit",
    "better-auth:gen": "sst shell npx @better-auth/cli@latest generate",
    "typecheck": "sst shell -- tsc --noEmit",
    "check": "npx ultracite@latest check",
    "fix": "npx ultracite@latest fix --unsafe"
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.908.0",
    "@effect/experimental": "^0.56.0",
    "@effect/platform": "^0.92.1",
    "@effect/platform-bun": "^0.81.1",
    "@effect/rpc": "^0.71.0",
    "@effect/sql": "^0.46.0",
    "@effect/sql-drizzle": "^0.45.0",
    "@effect/sql-pg": "^0.47.0",
    "@neondatabase/serverless": "^1.0.2",
    "@polar-sh/better-auth": "^1.1.9",
    "@polar-sh/sdk": "^0.37.0",
    "better-auth": "^1.3.27",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "effect": "latest",
    "hono": "^4.9.11",
    "ioredis": "^5.8.1",
    "nanoid": "^5.1.6",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@effect/build-utils": "^0.7.9",
    "@effect/vitest": "latest",
    "@types/pg": "^8.15.5",
    "@types/ws": "^8.18.1",
    "tsx": "^4.20.6"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
