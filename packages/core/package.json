{
  "name": "@lax-db/core",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./*": "./src/*.ts"
  },
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "effect": "latest"
  },
  "devDependencies": {
    "@effect/build-utils": "^0.7.7",
    "@effect/language-service": "^0.1.0",
    "@effect/vitest": "latest",
    "@types/node": "^22.5.2",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "tsx": "^4.17.0",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}